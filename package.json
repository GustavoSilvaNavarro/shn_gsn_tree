{
  "name": "shn_gsn_tree",
  "version": "1.0.0",
  "description": "Tree service api.",
  "main": "src/index.ts",
  "scripts": {
    "build": "docker build .",
    "start": "ts-node src/index.ts | ./node_modules/.bin/bunyan",
    "start:prod": "npm run start",
    "dev": "nodemon src/index.ts | ./node_modules/.bin/bunyan",
    "clear-cache": "jest --clear-cache",
    "test": "jest -c jest.config.ts",
    "test:int": "jest -c jest.integration.config.ts -i",
    "test:int:local": "set -a && source .env && npm run test:int",
    "wait:integration": "./wait-for.sh http://public_api:8080/healthz npm run integration",
    "integration": "set -a && source testing.env && npm run test:int",
    "lint": "npx eslint src/ tests/",
    "lint:fix": "npx eslint src/ tests/ --fix",
    "format": "npx prettier --write src tests",
    "check": "npx prettier --check src tests",
    "prepare": "husky install",
    "migration:up": "npx node-pg-migrate up",
    "migration:down": "npx node-pg-migrate down"

  },
  "author": "Gustavo Silva",
  "dependencies": {
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "bunyan": "^2.0.5",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "signal-exit": "^4.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.48.4",
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@tsconfig/recommended": "^1.0.10",
    "@types/body-parser": "^1.19.6",
    "@types/bunyan": "^1.8.11",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.2.0",
    "@types/signal-exit": "^3.0.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-extended": "^4.0.2",
    "jest-when": "^3.7.0",
    "nodemon": "^3.1.9",
    "npq": "^3.11.2",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.1",
    "typescript-transform-paths": "^3.5.5"
  }
}
